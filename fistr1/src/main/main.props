  <ItemDefinitionGroup>
    <ClCompile>
      <PreprocessorDefinitions>_UNICODE;UNICODE;USE_GKREGEX;_GK_ERROR_C_;_WINDOWS;__WIN32__;_CRT_SECURE_NO_WARNINGS;HECMW_SERIAL;%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
    <PostBuildEvent>
      <Command>postbuild_FemSolver2</Command>
    </PostBuildEvent>
    <Link>
      <AdditionalDependencies>FemSolver2\ffistr1.lib;FemSolver2\fhecmw1.lib;FemSolver2\hecmw1.lib;FemSolver2\libfistr.lib;FemSolver2\fistr_main.lib;%(AdditionalDependencies)</AdditionalDependencies>
