    <OutDir>$(Platform)\$(Configuration)\</OutDir>
    <IncludePath>$(WELSIM_LIBPACK)\include;$(WELSIM_SRC)\solver\WelSim.FemSolver2\hecmw1\include;$(WELSIM_SRC)\solver\WelSim.FemSolver2;$(IncludePath)</IncludePath>
